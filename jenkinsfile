pipeline {
    agent any

    environment {
        PATH = "$PATH:/usr/bin/node"
    }

    stages {
        stage('GetCode') {
            steps {
                git branch: 'master', url: 'https://github.com/sambit985/api-test.git'
            }
        }

        stage('Build') {
            steps {
                sh 'npm install'
            }
        }


  stage('deploy'){
     steps{
        rsync -azh --info=progress2 --info=name0 -e "ssh -i /var/lib/jenkins/.ssh/id_rsa"  /var/lib/jenkins/workspace/ssh-new/ ubuntu@3.81.114.150:/www/
       }
     }

    }
}
